---
import significado from "./data/significado.json";
---
<div class="flex flex-col-reverse content-center gap-y-5 mt-16 mb-16 md:px-24 md:py-24 md:items-center md:flex-row-reverse">

  <form class="md:w-1/2">

      <div class="flex flex-wrap justify-center">

        <div class="">
          <label class="block w-96 md:w-full text-gray-700 text-xm mb-2" for="grid-first-name">
            Nombre
          </label>
          <input class="appearance-none block w-full md:w-96 text-gray-700 border py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white" id="grid-first-name" type="text" placeholder="Tu nombre">
          <!--<p class="text-red-500 text-xm italic">Please fill out this field.</p>  -->
        </div>

        <div class="flex flex-wrap md:gap-x-5 md:gap-y-3 justify-center">
          <div class="">
            <label class="block w-96 md:w-auto text-gray-700 text-xm mb-2" for="grid-state">
              Edad
            </label>
            <div class="relative">
              <select class="block appearance-none w-full md:w-96 border border-gray-200 text-gray-700 py-3 px-4 pr-8 rounded-xl leading-tight focus:outline-none focus:bg-white focus:border-gray-500" id="grid-state">
                {significado.age.sort().map((age) => (
                  <option>{age}</option>
                ))}
              </select>
              <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700">
                <svg class="fill-current h-4 w-4" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20"><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z"/></svg>
              </div>
            </div>
          </div>

          <div class="">
            <label class="block w-96 md:w-auto text-gray-700 text-xm mb-2" for="grid-state">
              Menopause
            </label>
            <div class="relative">
              <select class="block appearance-none w-full md:w-96 border border-gray-200 text-gray-700 py-3 px-4 pr-8 rounded-xl leading-tight focus:outline-none focus:bg-white focus:border-gray-500" id="grid-state">
                {significado.menopause.sort().map((menopause) => (
                  <option>{menopause}</option>
                ))}
              </select>
              <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700">
                <svg class="fill-current h-4 w-4" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20"><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z"/></svg>
              </div>
            </div>
          </div>

          <div class="">
            <label class="block w-96 md:w-auto text-gray-700 text-xm mb-2" for="grid-state">
              Tumor size
            </label>
            <div class="relative">
              <select class="block appearance-none w-full md:w-96 border border-gray-200 text-gray-700 py-3 px-4 pr-8 rounded-xl leading-tight focus:outline-none focus:bg-white focus:border-gray-500" id="grid-state">
                {significado.tumorSize.sort().map((size) => (
                  <option>{size}</option>
                ))}
              </select>
              <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700">
                <svg class="fill-current h-4 w-4" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20"><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z"/></svg>
              </div>
            </div>
          </div>

          <div class="">
            <label class="block w-96 md:w-auto text-gray-700 text-xm mb-2" for="grid-state">
              inv-nodes
            </label>
            <div class="relative">
              <select class="block appearance-none w-full md:w-96 border border-gray-200 text-gray-700 py-3 px-4 pr-8 rounded-xl leading-tight focus:outline-none focus:bg-white focus:border-gray-500" id="grid-state">
                {significado.invNodes.sort().map((invNodes) => (
                  <option>{invNodes}</option>
                ))}
              </select>
              <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700">
                <svg class="fill-current h-4 w-4" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20"><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z"/></svg>
              </div>
            </div>
          </div>

          <div class="">
