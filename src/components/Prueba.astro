                {significado.nodeCaps.sort().map((nodeCaps) => (
                  <option>{nodeCaps}</option>
                ))}
              </select>
              <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700">
                <svg class="fill-current h-4 w-4" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20"><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z"/></svg>
              </div>
            </div>
          </div>

          <div class="">
            <label class="block w-96 md:w-auto text-gray-700 text-xm mb-2" for="grid-state">
              deg-malig
            </label>
            <div class="relative">
              <select class="block appearance-none w-full md:w-96 border border-gray-200 text-gray-700 py-3 px-4 pr-8 rounded-xl leading-tight focus:outline-none focus:bg-white focus:border-gray-500" id="grid-state">
